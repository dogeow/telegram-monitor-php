{
    "name": "telegram-monitor/php-client",
    "description": "PHP Telegram用户监听程序",
    "type": "project",
    "require": {
        "php": "^8.1",
        "danog/madelineproto": "^8.0",
        "monolog/monolog": "^3.0",
        "symfony/console": "^6.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "TelegramMonitor\\": "src/"
        }
    },
    "scripts": {
        "start": "php bin/monitor.php",
        "setup": "php bin/setup.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 